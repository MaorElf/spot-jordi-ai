{
  "name": "spot-jordi-ai",
  "version": "0.0.0",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "npm-install": "npm install --legacy-peer-deps",
    "start": "ng serve --proxy-config config/proxy.config.js",
    "test": "ng test",
    "coverage-report": "ng test --code-coverage --no-watch --browsers ChromeHeadless",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "npm update @spotinst/eslint-plugin-spot; eslint --fix .",
    "lint:diff": "npm update @spotinst/eslint-plugin-spot; eslint -c .eslintrc.diff.js .",
    "lint:diff:fix": "ESLINT_PLUGIN_DIFF_COMMIT=main eslint -c .eslintrc.diff.js . --fix",
    "serve": "ng serve",
    "serve-with-local-client-backend": "node config/serveWithLocalClientBackend.js",
    "build:prod": "ng build --configuration production",
    "build:dev": "ng build",
    "build:dev:watch": "npm run build:dev -- --watch",
    "analyize-bundle": "ng build --configuration production --stats-json; webpack-bundle-analyzer build/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.2.5",
