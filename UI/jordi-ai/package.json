    "@ngrx/store-devtools": "15.4.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@spotinst/spot-client-core-services-types": "^6.4.1",
    "@spotinst/spot-client-shared": "^38.18.2",
    "ace-builds": "1.15.3",
    "ag-grid-angular": "26.2.0",
